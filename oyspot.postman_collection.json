	"event": [
		{
			"listen": "prerequest",
			"script": {
				"id": "75fb8867-5476-4c77-a372-8f963bcf7932",
				"type": "text/javascript",
				"exec": [
					"let host = postman.getEnvironmentVariable('host');",
					"let login = postman.getEnvironmentVariable('login');",
					"let password = postman.getEnvironmentVariable('password');",
					"",
					"const loginRequest = {",
					"  url: host+'/auth/login',",
					"  method: 'POST',",
					"  header: 'Content-Type: application/json',",
					"  body: {",
					"    mode: 'raw',",
					"    raw: JSON.stringify({ login:login, password:password})",
					"  }",
					"};",
					"pm.sendRequest(loginRequest, function (err, res) {",
					"    ",
					"    if(err){",
					"        console.log(err);",
					"    }else{",
					"        var jsonData = res.json();",
					"        postman.setEnvironmentVariable(\"access_token\", jsonData.access_token);",
					"    }",
					"   ",
					"});"
				]
			}
		},
